<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>电商冷链物流</title>
  <link rel="stylesheet" href="../css/common.css">
  <link rel="stylesheet" href="../css/usecenter.css">
  <link rel="stylesheet" href="../publicJs/element_ui/element.css">
  <style>
    #p1 {
      display: block !important;
      height: 580px;
    }

    .navCont {
      width: 1200px;
      position: relative;
    }

    .line {
      position: absolute;
      top: 18%;
      left: 16%;
    }

    #content {
      width: 840px !important;
    }

    .newline {
      height: 50px;
      line-height: 50px;
      font-size: 14px;
      font-weight: normal;
      font-stretch: normal;
      letter-spacing: 0px;
      color: #333333;
      display: flex;
      margin-bottom: 20px;
      position: absolute;
      top: 90%;
      left: 35%;
    }

    .newline lineTitle {
      text-align: right;
      width: 61px;
      margin-right: 15px;
    }

    .newline .btn_save,
    .btn_cancel {
      width: 100px;
      height: 40px;
      border-radius: 2px;
      font-size: 14px;
      font-weight: normal;
      font-stretch: normal;
      line-height: 40px;
      letter-spacing: 0px;
      color: #55595c;
      text-align: center;
      cursor: pointer;
      display: inline-block;
      margin-right: 20px;
    }

    .newline .btn_save a {
      color: #fff;
      background-color: #dfaf89;
    }

    .newline .btn_cancel {
      border: solid 1px #e8e8e8;
    }

    .top {
      width: 100%;
      min-width: 1200px;
      height: 81px;
      display: flex;
      justify-content: space-between;
      background-color: #333;
      position: relative;
      overflow: hidden;
    }

    .top::before {
      content: "";
      position: absolute;
      left: 35%;
      width: 215px;
      height: 100px;
      background-color: #fff;
      transform: translate(-52px, 40px) rotate(30deg);
    }

    .top::after {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      width: 35%;
      height: 81px;
      background-color: #fff;
    }

    .use_handler {
      /* margin-left: 600px; */
      display: flex;
      margin-top: 25px;
    }

    .arrow_banner {
      display: flex;
      justify-content: space-between;
      width: 1200px;
      height: 80px;
      margin: 0 auto;
      position: absolute;
      top: 50%;
      margin-top: -40px;
      left: 19%;
    }

    .arrow_banner span {
      /* display: block; */
      width: 45px;
      height: 80px;
      background-color: rgba(0, 0, 0, 0.3);
      line-height: 80px;
      text-align: center;
      cursor: pointer;
      display: none;
      transition: all 1s;
    }

    .arrow_banner span img {
      vertical-align: middle;
      border-style: none;
    }

    .banner:hover .arrow_left {
      display: block;

    }

    .banner:hover .arrow_right {
      display: block;
    }

    .arrow_left:hover,
    .arrow_left:active {
      background-color: rgba(0, 0, 0, 1);
    }

    /* 顶 */

    .con {
      width: 1200px;
      margin: 0 auto;
      display: flex;
      justify-content: space-between;
      z-index: 999;
    }

    .con .left {
      padding-top: 16px;
    }

    .el-row::after,
    .el-row::before {
      display: block;
    }

    .el-row {
      width: 1200px;
      margin: 0% auto;
      text-align: center;
    }

    .el-button--primary {
      color: #fff;
      background-color: #dfaf89;
      border-color: #dfaf89;
      margin: 20px 0 30px;
    }

    .el-button--primary:focus,
    .el-button--primary:hover {
      background: #ebbe9a;
      border-color: #dfaf89;
      color: #fff;
    }

    .el-button a {
      font-size: 16px;
    }


    .con_one {
      display: none;
    }

    .active {
      display: block;
    }
    .bt{
      display: none;
    }
    .a_bt{
      display: block;
    }
  </style>
</head>

<body>
  <div id="editor" v-cloak>
    <div class="top">
      <div class="con">
        <div class="left">
          <img src="../img/logo.png" alt="">
        </div>
        <div class="use_handler">
          <div class="btn_black">
            <a href="../index.html">返回首页</a>
          </div>
          <div class="btn_white">
            <a href="./usecenter.html">个人中心</a>
          </div>
        </div>
      </div>
    </div>
    <div class="head">
      <div class="center">
        电商冷链物流
      </div>
    </div>
    <div class="locate">
      <div class="center">
        <i class="icon"></i>
        <span>电商冷链物流</span>
      </div>
    </div>
    <div class="con_list">
      <div class="con_one active" id="d1">
        <div class="center band">
          <img src="../img/ｓ.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d2">
        <div class="center band">
          <img src="../img/sy2.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d3">
        <div class="center band">
          <img src="../img/sy3.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d4">
        <div class="center band">
          <img src="../img/4.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d5">
        <div class="center band">
          <img src="../img/5.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d6">
        <div class="center band">
          <img src="../img/sy5-1.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d7">
        <div class="center band">
          <img src="../img/sy6.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d8">
        <div class="center band">
          <img src="../img/sy7.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d9">
        <div class="center band">
          <img src="../img/sy8.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
      <div class="con_one" id="d10">
        <div class="center band">
          <img src="../img/sy9.jpg" alt="" style="width: 1200px; margin-bottom: 15px;">
        </div>
      </div>
    </div>
    <div>
      <el-row>
        <el-button type="primary" class="bt a_bt">
          <a href="#">下一步</a>
        </el-button>
      </el-row>
    </div>
    <div>
      <el-row>
        <el-button type="primary" class="bt">
          <a href="./usecenter.html">完成</a>
        </el-button>
      </el-row>
    </div>
  </div>
  <script src="../publicJs/element_ui/vue.js"></script>
  <script src="../publicJs/element_ui/element.js"></script>
  <script src="../publicJs/jquery.js"></script>
   <script>
    var n = document.getElementsByClassName("con_one").length;
    var lt = document.getElementsByClassName("con_one").length
    var i = 0
    var index = $(this).index(".con_one");
    console.log(index,n);
    $(".el-button--primary").click(function () {
      if (i < n) {
        i++;
        $(".con_one").eq(i).addClass('active');
        $(".con_one").eq(i - 1).removeClass('active');
      }else if(i > n)
      {
        console.log($(".con_one").eq(i).style.display);
        $(".con_one").eq(i).style.display="none";
        $(".con_one").eq(i - 1).style.display="block";
      }
    });
    $(".el-button--primary").click(function () {
      if(index >= n-2){
      $(".bt").eq(0).removeClass('a_bt');
      $(".bt").eq(1).addClass('a_bt');
    }else if(index < n){
      index++;
      $(".bt").eq(0).addClass('a_bt');
      $(".bt").eq(1).removeClass('a_bt');
    }
  })
  </script>

</body>
</html>